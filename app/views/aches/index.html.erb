<p id="notice"><%= notice %></p>

<h1>Listing Aches</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @aches.each do |ach| %>
      <tr>
        <td><%= link_to 'Show', ach %></td>
        <td><%= link_to 'Edit', edit_ach_path(ach) %></td>
        <td><%= link_to 'Destroy', ach, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Ach', new_ach_path %>
